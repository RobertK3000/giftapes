<div class="nested-fields">
    <%= f.input :youtube_url, label: "Youtube url", placeholder: "Use a share link, e.g., https://youtu.be/s732BigTxZk" %>
    <%= f.input :start_time, label: "Start time (seconds)"  %>
    <%= f.input :end_time, label: "End time (seconds)" %>
    <%= link_to_remove_association "Remove this track", f, class:"btn btn-info btn-txt"%>
</div>
