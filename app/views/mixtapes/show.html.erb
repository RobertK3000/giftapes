  <div class="cassette-controls-container">
    <div class="js-mixtape-youtube-player" data-playlist-videos= <%= @tracks_json %>>
      <div class="js-video-controls">
        <button class="js-play-pause cassette-control-button" type="button" onclick="play()">
          <i class="fas fa-play"></i>
        </button>
        <button class="js-pause cassette-control-button" type="button">
          <i class="fas fa-pause"></i>
        </button>

        <button class="js-rewind cassette-control-button" type="button">
          <i class="fas fa-backward"></i>
        </button>

        <button class="js-fast-forward cassette-control-button" type="button">
          <i class="fas fa-forward"></i>
        </button>
      </div>
    
      <div id="player"></div>
